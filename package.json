{
  "name": "@weskerjs/supergen",
  "version": "0.0.9",
  "description": "Super generator for AdonisJS",
  "private": false,
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run clean && tsc && npm run copyfiles",
    "copyfiles": "copyfiles -u 1 templates/**/* build/templates",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "author": "Max Diamond",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@types/node": "^20.1.1",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "mrm": "^4.1.14",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/ace": "^11.3.1",
    "ejs": "^3.1.10",
    "fs": "^0.0.1-security"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.9.0"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "16.13.1",
    "probotApps": [],
    "runGhActionsOnWindows": false
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "commands": [
      "@adonisjs/core/build/commands/index.js"
    ]
  },
  "files": [
    "build"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weskerjs/supergen.git"
  },
  "bugs": {
    "url": "https://github.com/weskerjs/supergen/issues"
  },
  "homepage": "https://github.com/weskerjs/supergen#readme"
}
